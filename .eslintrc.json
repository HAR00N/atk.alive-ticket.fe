{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "vue/order-in-components": ["error"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 120
      }
    ]
  }
}
